/boot		emmc	/dev/block/bootdevice/by-name/boot		flags=backup=1
/recovery	emmc	/dev/block/bootdevice/by-name/recovery	flags=backup=1
/system		ext4	/dev/block/bootdevice/by-name/system	flags=backup=1
/system_image   emmc    /dev/block/bootdevice/by-name/system
/data		ext4	/dev/block/bootdevice/by-name/userdata	flags=encryptable=/dev/block/bootdevice/by-name/encrypt
/cache		ext4	/dev/block/bootdevice/by-name/cache
/firmware	vfat 	/dev/block/bootdevice/by-name/modem	flags=backup=0;display="Firmware";mounttodecrypt
/firmware_image	emmc 	/dev/block/bootdevice/by-name/modem	flags=backup=1;display="Firmware_Image"
/factory	emmc 	/dev/block/bootdevice/by-name/factory	flags=backup=1;subpartitionof=/firmware_image
/efs1		emmc	/dev/block/bootdevice/by-name/modemst1	flags=backup=1;display="EFS"
/efs2		emmc	/dev/block/bootdevice/by-name/modemst2	flags=backup=1;subpartitionof=/efs1
/efs3		emmc	/dev/block/bootdevice/by-name/fsg		flags=backup=1;subpartitionof=/efs1
/external_sd	vfat	/dev/block/mmcblk1p1 /dev/block/mmcblk1			flags=display="Micro SDCard";storage;wipeingui;removable;encryptable=/dev/block/bootdevice/by-name/encrypt
/usb-otg	vfat	/dev/block/sda1 /dev/block/sda				flags=display="USB-OTG";storage;wipeingui;removable
/misc		emmc    /dev/block/bootdevice/by-name/misc		flags=backup=1;display="BL unlock state"
/rmisc		emmc    /dev/block/bootdevice/by-name/misc		flags=backup=1;subpartitionof=/misc
/devinfo	emmc    /dev/block/bootdevice/by-name/devinfo		flags=backup=1;subpartitionof=/misc
/persist	emmc    /dev/block/bootdevice/by-name/persist		flags=backup=1;subpartitionof=/misc
/persistent	emmc    /dev/block/bootdevice/by-name/persistent		flags=backup=1;subpartitionof=/misc
/drm		emmc    /dev/block/bootdevice/by-name/drm		flags=backup=1;subpartitionof=/misc
/sns		emmc    /dev/block/bootdevice/by-name/sns		flags=backup=1;subpartitionof=/misc
/bl		emmc 	/dev/block/bootdevice/by-name/aboot        flags=backup=1;display="Bootloader"
/bl2		emmc    /dev/block/bootdevice/by-name/rpm        	flags=backup=1;subpartitionof=/bl
/bl3		emmc    /dev/block/bootdevice/by-name/pmic        	flags=backup=1;subpartitionof=/bl
/bl4		emmc    /dev/block/bootdevice/by-name/sdi        	flags=backup=1;subpartitionof=/bl
/bl5		emmc    /dev/block/bootdevice/by-name/laf        	flags=backup=1;subpartitionof=/bl
/bl6		emmc    /dev/block/bootdevice/by-name/tz        	flags=backup=1;subpartitionof=/bl
/bl7		emmc    /dev/block/bootdevice/by-name/hyp        	flags=backup=1;subpartitionof=/bl
/bl8		emmc    /dev/block/bootdevice/by-name/sbl1        	flags=backup=1;subpartitionof=/bl
/carrier	emmc    /dev/block/bootdevice/by-name/carrier      flags=backup=1;display="Carrier"
/cust		emmc    /dev/block/bootdevice/by-name/cust      	flags=backup=1;display="Carrier"
